{
  "name": "soulstones",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "create": "npx tsx bin/create.ts",
    "info": "npx tsx bin/info.ts",
    "list": "npx tsx bin/list.ts",
    "delete": "npx tsx bin/delete.ts",
    "claim": "npx tsx bin/claim.ts",
